<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GradingController">
   <children>
      <Button layoutX="229.0" layoutY="58.0" mnemonicParsing="false" onAction="#importStudentButtonPushed" text="Import Student" />
      <Button layoutX="447.0" layoutY="58.0" mnemonicParsing="false" onAction="#editStudentButtonPushed" text="Edit Student" />
      <TableView fx:id="gradeTableView" layoutX="100.0" layoutY="390.0" prefHeight="250.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="gradeBUIDColumn" prefWidth="75.0" text="BUID" />
          <TableColumn fx:id="gradeFirstNameColumn" prefWidth="75.0" text="First Name" />
            <TableColumn fx:id="gradeMiddleNameColumn" prefWidth="75.0" text="Middle Name" />
            <TableColumn fx:id="gradeLastNameColumn" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="gradeScoreColumn" onEditCommit="#changeGradeScoreCellEvent" prefWidth="75.0" text="Score" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="334.0" layoutY="25.0" text="Student Management" />
      <TableView fx:id="generalTableView" layoutX="100.0" layoutY="135.0" onMouseClicked="#userClickedOnGeneralTable" prefHeight="192.0" prefWidth="241.0">
        <columns>
          <TableColumn fx:id="generalTypeColumn" onEditCommit="#changeGeneralTypeCellEvent" prefWidth="75.0" text="Type" />
          <TableColumn fx:id="generalPercentageColumn" onEditCommit="#changeGeneralPercentageCellEvent" prefWidth="75.0" text="Percentage" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TableView fx:id="detailedTableView" layoutX="373.0" layoutY="135.0" onMouseClicked="#userClickOnDetailedTable" prefHeight="192.0" prefWidth="327.0">
        <columns>
          <TableColumn fx:id="detailedTypeColumn" onEditCommit="#changeDetailedTypeCellEvent" prefWidth="75.0" text="Type" />
          <TableColumn fx:id="detailedPercentageColumn" onEditCommit="#changeDetailedPercentageCellEvent" prefWidth="75.0" text="Percentage" />
            <TableColumn fx:id="detailedTotalScoreColumn" onEditCommit="#changeDetailedScoreCellEvent" prefWidth="75.0" text="Total Score" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="100.0" layoutY="102.0" text="General Criteria:" />
      <Label layoutX="373.0" layoutY="102.0" text="Detailed Criteria:" />
      <Button layoutX="293.0" layoutY="344.0" mnemonicParsing="false" onAction="#saveGeneralCriteriaButton" text="Save" />
      <Button layoutX="652.0" layoutY="344.0" mnemonicParsing="false" onAction="#saveDetailedCriteriaButton" text="Save" />
      <Button layoutX="652.0" layoutY="654.0" mnemonicParsing="false" onAction="#saveGradeScoreButton" text="Save" />
   </children>
</AnchorPane>
